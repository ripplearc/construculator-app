<?xml version="1.0" encoding="UTF-8"?>
<mutations version="1.0">
   <files>
        <file>lib/libraries/logging/app_logger.dart</file>
    </files>
    <rules>
        <regex pattern="(_tag|_emojiPrefix)\s*=\s*(''|&quot;[^&quot;]*&quot;|'[^']*');" id="app_logger.field_assignment.mutateStringLiteral">
            <mutation text="$1 = &quot;mutatedString&quot;;"/>
        </regex>
        <regex pattern="(\w+\.isNotEmpty)" id="generic.boolean.invertIsNotEmpty">
            <mutation text="$1.isEmpty"/>
        </regex>
        <regex pattern="(AppLoggerImpl\._private\([^)]*?)(''|&quot;[^&quot;]*&quot;|'[^']+')([^)]*?\))" id="app_logger.constructor.mutateStringArgument">
            <mutation text="$1&quot;mutatedPrivateArg&quot;$3"/>
        </regex>
        <regex pattern="AppLoggerImpl\._private\((newTag), (_emojiPrefix), (_internalLogger)\);" id="app_logger.tag.swapArguments">
            <mutation text="AppLoggerImpl._private($2, $1, $3);"/>
        </regex>
        <regex pattern="(_internalLogger)\.i\(" id="app_logger.internal_logger.change_i_to_w">
            <mutation text="$1.w("/>
        </regex>
        <regex pattern="(_internalLogger\.\w+\()_formatMessage\(([^)]+)\)([^)]*\))" id="app_logger.logging.removeFormatMessageCall" dotAll="true">
            <mutation text="$1$2$3"/>
        </regex>
    </rules>
    <commands>
           <command group="app_logger" expected-return="0">flutter test test/units/libraries/logging/app_logger_test.dart</command>
    </commands>
    <threshold failure="85">
        <rating over="85" name="A"/>
        <rating over="75" name="B"/>
        <rating over="65" name="C"/>
        <rating over="55" name="D"/>
        <rating over="35" name="E"/>
        <rating over="0" name="F"/>
    </threshold>
</mutations> 